  "license": "MIT",
  "scripts": {
    "dev": "concurrently -n \"Frontend,Backend\" -c \"blue,green\" \"npm run dev:dashboard\" \"npm run dev:api\"",
    "build": "nx run-many -t build --projects=dashboard,api --parallel",
    "dev:dashboard": "nx serve dashboard",
    "dev:api": "nx serve api --skip-nx-cache",
    "build:dashboard": "nx build dashboard",
    "build:api": "nx build api --skip-nx-cache",
    "db:up": "cd apps/api && npx sequelize db:drop && npx sequelize db:create && npx sequelize db:migrate && npx sequelize db:seed:all",
    "db:drop": "cd apps/api && npx sequelize db:drop",
    "db:create": "cd apps/api && npx sequelize db:create",
    "create-migration:dev": "cd apps/api && npx sequelize migration:generate --name",
    "migrate:dev": "cd apps/api && npx sequelize db:migrate",
    "migrate:undo:dev": "cd apps/api && npx sequelize db:migrate:undo",
    "migrate:undo:all:dev": "cd apps/api && npx sequelize db:migrate:undo:all",
    "migrate:status:dev": "cd apps/api && npx sequelize db:migrate:status",
    "create-seeder:dev": "cd apps/api && npx sequelize seed:generate --name",
    "seed:dev": "cd apps/api && npx sequelize db:seed:all",
    "seed:undo:dev": "cd apps/api && npx sequelize db:seed:undo:all",
    "generate:api:resource": "node scripts/generate-api-resource.ts",
    "generate:ui:resource": "node scripts/generate-ui-resource.ts"
  },
  "private": true,
